  "scripts": {
    "build": "webpack --config script/webpack.build.js",
    "lint": "eslint --ext .vue --ext .js src/ --fix",
    "analyz": "npm_config_report=true npm run build"
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/epage-team/epage-core.git"
  },
  "keywords": [
    "epage",
    "core"
  ],
  "bugs": {
    "url": "https://github.com/epage-team/epage-core/issues"
  },
  "homepage": "https://github.com/epage-team/epage-core#readme",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && git add .",
      "commit-msg": "node script/verify-commit-msg.js"
    }
  },
  "peerDependencies": {
    "vue": ">=2.5",
    "vuex": ">=3.1"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "clean-webpack-plugin": "^2.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.0.1",
    "husky": "^4.2.3",
    "uglifyjs-webpack-plugin": "^1.0.0-rc.0",
    "vuex": "^3.1.1",
    "vue": "^2.5",
    "webpack": "^4.35.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "engines": {
    "node": ">= 8.10.0",
    "npm": ">= 4.0.0"
